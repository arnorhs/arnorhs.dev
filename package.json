  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "astro",
      "posts",
      "post-exporter"
    ]
  },
  "scripts": {
    "posts": "yarn workspace @arnorhs/posts",
    "post-exporter": "yarn workspace @arnorhs/post-exporter",
    "astro": "yarn workspace @arnorhs/astro",

    "build-posts": "yarn posts build && TARGET_DIR=astro/src/gen yarn post-exporter build",
    "export": "yarn build-posts && yarn astro build",
    "dev": "yarn build-posts && yarn astro dev"
  },
  "dependencies": {}
}
